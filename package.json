{
    "name": "notes",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint",
        "start": "cross-env REQ_LOG_FORMAT=common DEBUG=notes:* USER_SERVICE_URL=http://localhost:3001 node ./app.js",
        "server1": "cross-env REQ_LOG_FORMAT=common DEBUG=notes:* PORT=3002 node ./app.js",
        "server2": "cross-env REQ_LOG_FORMAT=common DEBUG=notes:* PORT=3003 node ./app.js",
        "theme": "npm run theme-css && npm run theme-min-css",
        "theme-css": "wget https://bootswatch.com/5/superhero/bootstrap.css -O theme/superhero/bootstrap.css",
        "theme-min-css": "wget https://bootswatch.com/5/superhero/bootstrap.min.css -O theme/superhero/bootstrap.min.css",
        "build": "npm run lint && npm run theme",
        "sqlite3-setup": "sqlite3 ./data/notes.sqlite3 --init ./models/schema-sqlite3.sql"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bootstrap": "^5.3.3",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "express": "~4.21.0",
        "express-session": "^1.18.1",
        "feather-icons": "^4.29.2",
        "fs-extra": "^11.2.0",
        "hbs": "~4.2.0",
        "js-yaml": "^4.1.0",
        "level": "^8.0.1",
        "mongodb": "^6.9.0",
        "morgan": "~1.10.0",
        "passport": "^0.7.0",
        "passport-local": "^1.0.0",
        "passport-twitter": "^1.0.4",
        "rotating-file-stream": "^3.2.5",
        "sequelize": "^6.37.4",
        "session-file-store": "^1.5.0",
        "sqlite3": "^5.1.7",
        "superagent": "^10.1.0"
    },
    "devDependencies": {
        "eslint": "^9.11.1",
        "globals": "^15.9.0"
    }
}
